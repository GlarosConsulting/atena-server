  "engines": {
    "node": ">= 12.18.4",
    "npm": ">= 6.14.8",
    "yarn": ">= 1.22.5"
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --no-notify --ignore-watch node_modules src/server.ts",
