    "@typescript-eslint/parser": "^3.4.0",
    "@zerollup/ts-transform-paths": "^1.7.17",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
