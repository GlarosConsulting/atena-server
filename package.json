    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.5.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "read-excel-file": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
